<!--
	Relation:

	0x,0y      100x,0y
	.__________.
	|         |
	|   50,50 |
	|   / \   |
	|  /   \  |
	| /     \ |
	|_________|
	0,100y     100x,100y

	M x y        Move (x,y)
	M x y x y    Move (x,y) Draw next point to (x, y)

  -->

<vector xmlns:android="http://schemas.android.com/apk/res/android"

android:width="100dp"
android:height="100dp"
android:viewportHeight="100"
android:viewportWidth="100">
	<!-- Shadowed Underlay -->
	<!-- Top 1-->
	<path
		android:name="line"
		android:pathData="M 50 11.5 50 18.2"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Top 2-->
	<path
		android:name="line"
		android:pathData="M 50 22.5 50 29"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Top 3-->
	<path
		android:name="line"
		android:pathData="M 50 34 50 45"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Bottom -->
	<path
		android:name="line"
		android:pathData="M 50 55 50 58"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Left -->
	<path
		android:name="line"
		android:pathData="M 45 50 40 50"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Right -->
	<path
		android:name="line"
		android:pathData="M 55 50 60 50"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5" />
	<!-- Center -->
	<path
		android:name="line"
		android:pathData="M 49.7 50 50.3 50"
		android:strokeColor="@color/textShadow2"
		android:strokeWidth="0.5"/>

	<!-- Non-Shadowed Overlay -->
	<!-- Top 1-->
	<path
		android:name="line"
		android:pathData="M 50 11.5 50 18.2"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Top 2-->
	<path
		android:name="line"
		android:pathData="M 50 22.5 50 29"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Top 3-->
	<path
		android:name="line"
		android:pathData="M 50 34 50 45"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Bottom -->
	<path
		android:name="line"
		android:pathData="M 50 55 50 58"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Left -->
	<path
		android:name="line"
		android:pathData="M 40 50 45 50"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Right -->
	<path
		android:name="line"
		android:pathData="M 55 50 60 50"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
	<!-- Center -->
	<path
		android:name="line"
		android:pathData="M 49.7 50 50.3 50"
		android:strokeColor="#FFF"
		android:strokeWidth="0.2" />
</vector>
