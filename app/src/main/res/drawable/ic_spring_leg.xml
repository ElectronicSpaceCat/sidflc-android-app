<!--
	Relation:

	0x,0y      100x,0y
	.__________.
	|         |
	|   50,50 |
	|   / \   |
	|  /   \  |
	| /     \ |
	|_________|
	0,100y     100x,100y

	M x y        Move (x,y)
	M x y x y    Move (x,y) Draw next point to (x, y)

  -->

<vector xmlns:android="http://schemas.android.com/apk/res/android"
	android:width="1dp"
	android:height="125dp"
	android:viewportHeight="125"
	android:viewportWidth="1">
	<path
		android:name="line"
		android:pathData="M 0 0 0 125"
		android:strokeColor="#FFF"
		android:strokeWidth="1" />
</vector>
