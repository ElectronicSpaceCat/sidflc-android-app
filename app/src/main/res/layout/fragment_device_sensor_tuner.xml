<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/graph_window"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_margin="8dp">

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:text="Range (mm) / time"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <com.android.app.utils.plotter.CanvasPlotter
            android:id="@+id/graph_view_box"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/graph_view_plotter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/graph_view_plotter">

        </com.android.app.utils.plotter.CanvasPlotter>

        <com.android.app.utils.plotter.CanvasPlotter
            android:id="@+id/graph_view_plotter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </com.android.app.utils.plotter.CanvasPlotter>

        <TextView
            android:id="@+id/loading_configs"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/textShadow2"
            android:gravity="center"
            android:text="@string/loading_configs"
            android:textAlignment="center"
            android:textColor="@color/text_primary_color_selector"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="15dp"
            android:gravity="center"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/section_graph_controls"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:id="@+id/linearLayout4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="2dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TableRow
                        android:id="@+id/tableRow2"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/tableLayout4">

                        <TextView
                            android:id="@+id/queue_size"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Sample Size"
                            android:textColor="@color/textprimary" />

                        <TextView
                            android:id="@+id/queue_size_value"
                            android:layout_width="32dp"
                            android:layout_height="wrap_content"
                            android:text="@string/value_unknown"
                            android:textColor="@color/lightgray" />

                        <ImageView
                            android:id="@+id/btn_edit_queue_size"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            app:srcCompat="@drawable/ic_edit"
                            tools:ignore="ContentDescription" />
                    </TableRow>

                    <TableRow
                        android:id="@+id/tableRow"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tableRow2">

                        <TextView
                            android:id="@+id/increments"
                            android:layout_width="80dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="5dp"
                            android:text="Y Increment"
                            android:textColor="@color/textprimary" />

                        <TextView
                            android:id="@+id/increments_value"
                            android:layout_width="32dp"
                            android:layout_height="wrap_content"
                            android:text="@string/value_unknown"
                            android:textColor="@color/lightgray" />

                        <ImageView
                            android:id="@+id/btn_edit_increments"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            app:srcCompat="@drawable/ic_edit"
                            tools:ignore="ContentDescription" />
                    </TableRow>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="2dp"
                    app:layout_constraintEnd_toStartOf="@+id/linearLayout6"
                    app:layout_constraintStart_toEndOf="@+id/linearLayout4"
                    app:layout_constraintTop_toTopOf="@+id/linearLayout4">

                    <ImageButton
                        android:id="@+id/nav_up"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:srcCompat="@drawable/ic_keyboard_arrow_up_24"
                        tools:ignore="ContentDescription,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

                    <ImageButton
                        android:id="@+id/nav_down"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:srcCompat="@drawable/ic_keyboard_arrow_down_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="2dp"
                    app:layout_constraintEnd_toStartOf="@+id/linearLayout7"
                    app:layout_constraintStart_toEndOf="@+id/linearLayout3"
                    app:layout_constraintTop_toTopOf="@+id/linearLayout3">

                    <ImageButton
                        android:id="@+id/zoom_out"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:srcCompat="@drawable/ic_expand_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <ImageButton
                        android:id="@+id/zoom_in"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:srcCompat="@drawable/ic_contract_24"
                        tools:ignore="ContentDescription,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="5dp"
                    android:orientation="vertical"
                    android:padding="2dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                    <ImageButton
                        android:id="@+id/btn_auto_scale"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:srcCompat="@drawable/ic_fit_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <ImageButton
                        android:id="@+id/btn_plot_reset"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:padding="2dp"
                        app:srcCompat="@drawable/ic_rotation_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="5dp"
                android:background="@android:color/darker_gray"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                tools:layout_editor_absoluteX="46dp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/section_device_info"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/pwr_source_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/pwr_src_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Pwr Source"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/pwr_src"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/value_unknown"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/pwr_src_title" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/batt_status_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toStartOf="@+id/batt_level_info"
                    app:layout_constraintStart_toEndOf="@+id/pwr_source_info"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/batt_status_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Batt Status"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/batt_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/value_unknown"
                        android:textColor="@color/text_secondary_color_selector"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/batt_status_title" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/batt_level_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="@+id/range_info_adj"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/batt_level_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Batt Level (mV)"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/batt_level"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/value_unknown"
                        app:layout_constraintStart_toStartOf="@+id/batt_level_title"
                        app:layout_constraintTop_toBottomOf="@+id/batt_level_title" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:id="@+id/view3"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginVertical="5dp"
                    android:background="@android:color/darker_gray"
                    app:layout_constraintTop_toBottomOf="@+id/pwr_source_info" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/sensor_type_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/view3">

                    <TextView
                        android:id="@+id/textView28"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sensor"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/sensor_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/value_unknown"
                        app:layout_constraintStart_toStartOf="@+id/textView28"
                        app:layout_constraintTop_toBottomOf="@+id/textView28" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/sensor_status_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="@+id/batt_status_info"
                    app:layout_constraintTop_toBottomOf="@+id/view3">

                    <TextView
                        android:id="@+id/textView24"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sensor Status"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/sensor_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/value_unknown"
                        app:layout_constraintStart_toStartOf="@+id/textView24"
                        app:layout_constraintTop_toBottomOf="@+id/textView24" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:id="@+id/view7"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginVertical="5dp"
                    android:background="@android:color/darker_gray"
                    app:layout_constraintTop_toBottomOf="@+id/sensor_status_info"
                    tools:layout_editor_absoluteX="22dp" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/sensor_selector"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/view7">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Sensor Select"
                        android:textColor="@color/textprimary"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <RadioButton
                        android:id="@+id/btnSensor1"
                        style="@style/Widget.AppCompat.CompoundButton.RadioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:text="Carriage Pos"
                        android:textColor="@color/text_primary_color_selector"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/textView5"
                        app:layout_constraintTop_toBottomOf="@+id/textView5"
                        tools:ignore="TouchTargetSizeCheck" />

                    <RadioButton
                        android:id="@+id/btnSensor2"
                        style="@style/Widget.AppCompat.CompoundButton.RadioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:text="Height"
                        android:textAppearance="@style/MyPreferenceCategoryTitleTextAppearance"
                        android:textColor="@color/text_primary_color_selector"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/btnSensor1"
                        app:layout_constraintTop_toBottomOf="@+id/btnSensor1"
                        tools:ignore="TouchTargetSizeCheck" />

                    <androidx.appcompat.widget.SwitchCompat
                        android:id="@+id/switch_sensor_enable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Enable "
                        android:textAlignment="viewStart"
                        android:textColor="@color/text_primary_color_selector"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/btnSensor2" />

                    <androidx.appcompat.widget.SwitchCompat
                        android:id="@+id/switch_drift_comp_enable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Drift\nComp  "
                        android:textAlignment="viewStart"
                        android:textColor="@color/text_primary_color_selector"
                        app:layout_constraintEnd_toEndOf="@+id/switch_sensor_enable"
                        app:layout_constraintTop_toBottomOf="@+id/switch_sensor_enable" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/range_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toStartOf="@+id/range_info_adj"
                    app:layout_constraintStart_toEndOf="@+id/sensor_selector"
                    app:layout_constraintTop_toBottomOf="@+id/view7">

                    <TextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Range (mm)"
                        android:textColor="@color/textprimary"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TableLayout
                        android:id="@+id/tableLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:layout_constraintStart_toEndOf="@+id/textView9"
                        app:layout_constraintStart_toStartOf="@+id/textView9"
                        app:layout_constraintTop_toBottomOf="@+id/textView9">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/drift_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Drift"
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/drift_data"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/sd_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="S.D."
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/sd_data"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/raw_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Raw "
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/raw_data"
                                android:layout_width="65dp"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/filtered_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Avg"
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/filtered_data"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                        </TableRow>

                    </TableLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/range_info_adj"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/view7">

                    <TextView
                        android:id="@+id/textView25"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Adjustments"
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TableLayout
                        android:id="@+id/tableLayout2"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView25">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/ref_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Trgt. Ref "
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/ref_data"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                            <ImageView
                                android:id="@+id/btn_edit_ref"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                app:srcCompat="@drawable/ic_edit"
                                tools:ignore="ContentDescription" />

                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/sample_size_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Spl.Size  "
                                android:textColor="@color/text_primary_color_selector"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/sample_size_data"
                                android:layout_width="35dp"
                                android:layout_height="wrap_content"
                                android:text="@string/value_unknown"
                                android:textAlignment="viewStart"
                                android:textColor="@color/lightgray"
                                android:textSize="14sp" />

                            <ImageView
                                android:id="@+id/btn_edit_sample_size"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                app:srcCompat="@drawable/ic_edit"
                                tools:ignore="ContentDescription" />

                        </TableRow>

                    </TableLayout>


                </androidx.constraintlayout.widget.ConstraintLayout>



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/reset_sd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    app:layout_constraintStart_toStartOf="@+id/range_info"
                    app:layout_constraintTop_toBottomOf="@+id/range_info">

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Reset\nS.D."
                        android:textColor="@color/textprimary"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <ImageButton
                        android:id="@+id/btn_reset_sd"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView2"
                        app:srcCompat="@drawable/ic_rotation_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/reset_sensor_options"
                    android:layout_width="175dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/reset_sd">

                    <TextView
                        android:id="@+id/textView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Store\nConfigs"
                        android:textColor="@color/textprimary"
                        app:layout_constraintBottom_toTopOf="@+id/btn_store_configs"
                        app:layout_constraintStart_toStartOf="@+id/btn_store_configs"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="SpeakableTextPresentCheck" />

                    <ImageButton
                        android:id="@+id/btn_store_configs"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/btn_reset_sensor_default"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView17"
                        app:srcCompat="@drawable/ic_baseline_arrow_downward_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Reset\nDefault"
                        android:textColor="@color/textprimary"
                        app:layout_constraintBottom_toTopOf="@+id/btn_reset_sensor_default"
                        app:layout_constraintStart_toStartOf="@+id/btn_reset_sensor_default"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageButton
                        android:id="@+id/btn_reset_sensor_default"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/btn_reset_sensor_factory"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/btn_store_configs"
                        app:layout_constraintTop_toBottomOf="@+id/textView13"
                        app:srcCompat="@drawable/ic_rotation_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                    <TextView
                        android:id="@+id/textView15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Reset\nFactory"
                        android:textColor="@color/textprimary"
                        app:layout_constraintBottom_toTopOf="@+id/btn_reset_sensor_factory"
                        app:layout_constraintStart_toStartOf="@+id/btn_reset_sensor_factory"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageButton
                        android:id="@+id/btn_reset_sensor_factory"
                        style="@android:style/Widget.Holo.Light.ImageButton"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/btn_reset_sensor_default"
                        app:layout_constraintTop_toBottomOf="@+id/textView15"
                        app:srcCompat="@drawable/ic_rotation_24"
                        tools:ignore="ContentDescription,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="5dp"
                android:background="@android:color/darker_gray"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                tools:layout_editor_absoluteX="46dp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/section_config"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <include
                    android:id="@+id/config_header"
                    layout="@layout/item_config_header"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="wrap_content"
                    android:layout_height="270dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/config_header"
                    tools:listitem="@layout/item_config" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <View
                android:id="@+id/view6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginVertical="5dp"
                android:background="@android:color/darker_gray"
                app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
                tools:layout_editor_absoluteX="46dp" />

            <androidx.fragment.app.FragmentContainerView
                android:id="@+id/container_device_model"
                android:name="com.android.app.fragments.device.deviceModel.DeviceModelFragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:layout="@layout/fragment_device_model" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>